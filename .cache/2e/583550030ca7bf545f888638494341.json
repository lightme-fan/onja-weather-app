overflow-y: scroll;\r\n    }\r\n}","scss\\searchForm.scss":".search_container {\r\n    position: relative;\r\n}\r\n\r\n.search_form {\r\n    width: 100%;\r\n    height: 100%;\r\n    padding-bottom: 710px;\r\n    position: absolute;\r\n    transform:translateX(-700px);\r\n    margin-top: -47px;\r\n    padding-top: 4rem;\r\n    background-color: hsl(234deg 33% 17%);\r\n    -webkit-transform:translateX(-700px);\r\n    -moz-transform:translateX(-700px);\r\n    -ms-transform:translateX(-700px);\r\n    -o-transform:translateX(-700px);\r\n}\r\n\r\n.input_search {\r\n    background-color: transparent;\r\n    color: white;\r\n    border: 2px solid white;\r\n    padding: 4%;   \r\n}\r\n\r\n.button_search {\r\n    padding: 0.85rem 1.3%;\r\n    margin-left: 1rem;\r\n    cursor: pointer;\r\n}\r\n\r\n.search_button {\r\n    padding: 0.85rem 10%;\r\n    background-color: hsl(216deg 15% 52%);\r\n    color: #ffffff;\r\n    cursor: pointer;\r\n    font-size: 1rem;\r\n    line-height: 1;\r\n    border-radius: 5px;\r\n    -webkit-border-radius: 5px;\r\n    -moz-border-radius: 5px;\r\n    -ms-border-radius: 5px;\r\n    -o-border-radius: 5px;\r\n}\r\n\r\n.open {\r\n    transform:translateX(0);\r\n    -webkit-transform:translateX(0);\r\n    -moz-transform:translateX(0);\r\n    -ms-transform:translateX(0);\r\n    -o-transform:translateX(0);\r\n}\r\n\r\n.open ~ body {\r\n    overflow-y: hidden;\r\n}\r\n\r\n.cancel {\r\n    position: absolute;\r\n    top: 13px;\r\n    left: 90%;\r\n    cursor: pointer;\r\n}\r\n\r\n.remove {\r\n    transform:translateX(-700px);\r\n    -webkit-transform:translateX(-700px);\r\n    -moz-transform:translateX(-700px);\r\n    -ms-transform:translateX(-700px);\r\n    -o-transform:translateX(-700px);\r\n}\r\n\r\n.list_location {\r\n    list-style: none;\r\n    padding: 0;\r\n    margin: 2rem 6rem 2rem 0;\r\n}\r\n\r\n.location_value {\r\n    border: 1px solid;\r\n    padding: 1rem;\r\n    margin-top: 1rem;\r\n    cursor: pointer;\r\n}","scss\\todayWeather.scss":".weat