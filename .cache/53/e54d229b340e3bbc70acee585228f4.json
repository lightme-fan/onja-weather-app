mn":30}},{"generated":{"line":68,"column":35},"source":"components/useAppReducer.js","original":{"line":60,"column":11}},{"name":"setLocation","generated":{"line":68,"column":37},"source":"components/useAppReducer.js","original":{"line":60,"column":40}},{"generated":{"line":68,"column":48},"source":"components/useAppReducer.js","original":{"line":60,"column":11}},{"name":"fetchedDataByLocation","generated":{"line":68,"column":50},"source":"components/useAppReducer.js","original":{"line":60,"column":53}},{"generated":{"line":68,"column":71},"source":"components/useAppReducer.js","original":{"line":60,"column":11}},{"generated":{"line":68,"column":72},"source":"components/useAppReducer.js","original":{"line":60,"column":4}},{"generated":{"line":69,"column":0},"source":"components/useAppReducer.js","original":{"line":61,"column":1}},{"name":"useAppReducer","generated":{"line":71,"column":15},"source":"components/useAppReducer.js","original":{"line":63,"column":15}}],"sources":{"components/useAppReducer.js":"import React, { useReducer, useState, useEffect, useContext } from 'react'\r\nimport axios from 'axios'\r\n\r\nconst CORS_URL = 'https://cors-anywhere.herokuapp.com'\r\nconst KEY_URL = 'https://www.metaweather.com/api/location'\r\nconst LOCATION_URL = 'search/?query='\r\nconst LATTLONG = 'search/?lattlong='\r\n\r\n\r\nconst initialValue = {\r\n    isLoading: true,\r\n    weather: []\r\n}\r\n\r\nfunction reducer(state, action) {\r\n    switch (action.type) {\r\n        case 'FETCH_WEATHER_DATA': {\r\n            return {\r\n                ...state,\r\n                isLoading: false,\r\n                weather: action.data\r\n            }\r\n        }\r\n\r\n        default: {\r\n            return state\r\n        }\r\n    }\r\n    \r\n}\r\n\r\nfunction useAppReducer() {\r\n    const [ state, dispatch ] = useReducer(reducer, initialValue)\r\n    \r\n    const [location, setLocation ] = useState('London')\r\n    const [ lattLong, setLattLong ] = useState([])\r\n    \r\n    const fetchedDataByLocation = async () => {\r\n        \r\n        const res = await axios(`${CORS_URL}/${KEY_URL}/${LOCATION_URL}${location}`)\r\n        const locationData = res.data;\r\n        const lattlong = locationData.map(loc => loc.latt_long);\r\n\r\n        const diffRes = await axios(`${CORS_URL}/${KEY_URL}/${LATTLONG}${lattlong}`)\r\n        const lattlongData = diffRes.data\r\n        setLattLong(lattlongData)\r\n\r\n        const woeid = locationData.map(loc => loc.woeid);\r\n\r\n        const response = await axios(`${CORS_URL}/${KEY_URL}/${woeid}`)\r\n        dispatch({type: 'FETCH_WEATHER_DATA', data: response.data})\r\n\r\n    }\r\n    console.log(lattLong);\r\n    \r\n    useEffect(() => {\r\n        fetchedDataByLocation()\r\n    }, [])\r\n\r\n    return [ state, dispatch, location, setLocation, fetchedDataByLocation ]\r\n}\r\n\r\nexport default useAppReducer\r\n"},"lineCount":null}},"error":null,"hash":"dd333ea779f71b5889fcfafbd3ea289f","cacheData":{"env":{}}}