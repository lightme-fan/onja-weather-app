ather.js","original":{"line":38,"column":21}},{"generated":{"line":65,"column":3},"source":"components/NextWeather.js","original":{"line":24,"column":46}},{"generated":{"line":65,"column":4},"source":"components/NextWeather.js","original":{"line":23,"column":12}},{"generated":{"line":65,"column":5},"source":"components/NextWeather.js","original":{"line":22,"column":8}},{"generated":{"line":65,"column":6},"source":"components/NextWeather.js","original":{"line":21,"column":4}},{"generated":{"line":66,"column":0},"source":"components/NextWeather.js","original":{"line":44,"column":1}},{"name":"NextWeather","generated":{"line":68,"column":15},"source":"components/NextWeather.js","original":{"line":46,"column":15}}],"sources":{"components/NextWeather.js":"import React, { useContext, useState } from 'react'\nimport { Link } from 'react-router-dom'\n\nimport { Context } from './ContextProvider'\nimport DateFormat from './DateFormat'\nimport weatherImages from '../weatherImages'\nimport celciusToFahrenet from './celsuisToFarheneit'\n\nfunction NextWeather({onClick}) {\n    const { isLoading, weather, isTypeTempCelsius } = useContext(Context)\n\n    const [ nextDayWeather, setNextDayWeather ] = useState(null)\n\n    const day2 = !isLoading && weather && weather.consolidated_weather[1]\n    const day3 = !isLoading && weather && weather.consolidated_weather[2]\n    const day4 = !isLoading && weather && weather.consolidated_weather[3]\n    const day5 = !isLoading && weather && weather.consolidated_weather[4]\n    const day6 = !isLoading && weather && weather.consolidated_weather[5]\n    const weekWeather = [day2, day3, day4, day5, day6]\n    \n    return (\n        <>\n            <div className=\"next_weather\">\n                {!isLoading && weekWeather && weekWeather.map((day, i) => {\n                    const images = weatherImages.find(image => image.title === day.weather_state_name)        \n                    return (\n                        <div className=\"weather_this-day\" key={day.id}>\n                            <Link to={`/home/${day.id}`} className=\"weather_this-day-detail\" key={day.id}>\n                                <div className=\"day\">{i === 0 ? 'Tomorrow': DateFormat(day.applicable_date)}</div>\n                                <img src={images?.image} alt={images?.title}/>\n                                <div className=\"temperature\">\n                                    <div className=\"max_temp\">{!isTypeTempCelsius ? `${celciusToFahrenet(day.max_temp.toFixed(0))} ºF` : `${day.max_temp.toFixed(0)} ºC`}</div>\n                                    <div className=\"min_temp\">{!isTypeTempCelsius ? `${celciusToFahrenet(day.min_temp.toFixed(0))} ºF` : `${day.min_temp.toFixed(0)} ºC`}</div>\n                                </div>\n                            </Link>\n                        </div>\n                        )\n                    })\n                }\n            </div>\n            \n        </>\n    )\n}\n\nexport default NextWeather\n"},"lineCount":null}},"error":null,"hash":"9b43d521d0f3ba8185c5811e7dfced5e","cacheData":{"env":{}}}